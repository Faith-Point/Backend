{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend from Faith Point Project",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json && cp -R src/. dist/",
    "start": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/shared/http/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/shared/http/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "test": "jest --verbose --runInBand",
    "seed:generate": "ts-node-dev -r tsconfig-paths/register  src/shared/database/typeorm/seed/generateSeed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Faith-Point/Backend.git"
